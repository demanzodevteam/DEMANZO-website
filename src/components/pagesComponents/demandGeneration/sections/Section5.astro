---
import Accordian from "../../../common/Accordian"
import { API_URL } from "../../../../config/urls";

const res = await fetch(API_URL + 'blocks/499');
const data = await res.json();

const pageData = data[4];
const background = '[#FEF2F2]';
const colVal = [
  { value: 1 },
  { value: 1 },
  { value: 3 }
];
const combinedData={...pageData,colVal,background}

---
<!-- Section 5 -->
<Accordian category={combinedData} client:load/>                        