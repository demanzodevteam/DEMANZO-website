---
import { API_URL } from '../../../../config/urls';
import WhyYouNeedSection from '../../../common/WhatMattersForItCompanies';


const res = await fetch(API_URL + 'blocks/933');
const SectionData = await res.json();
const colVal = [
  { value: 1 },
  { value: 2 },
  { value: 4 }
];
const background = '#FFF5F4'
---

<!-------- Third Section ------->
<WhyYouNeedSection SectionData={SectionData[2]} colVal={colVal} background={background} client:load/>





