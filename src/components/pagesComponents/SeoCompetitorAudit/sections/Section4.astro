---
import CaseStudyCard from "../../../common/CaseStudyCard";
import { BASE_URL } from '../../../../config/urls';

const resond = await fetch(`${BASE_URL}wp-json/custom/v1/case-studies`);
const category = await resond.json();
---


<!-- ------------------section 4 --------------------- -->


<section class="py-20 px-4 lg:px-8 bg-gradient-to-b from-[#f8f8f8] to-white">
 <CaseStudyCard categories={category} client:load/>
</section>
