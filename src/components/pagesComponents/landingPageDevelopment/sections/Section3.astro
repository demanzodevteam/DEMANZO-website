---
import { API_URL } from '../../../../config/urls';
import ContentIntroSection from '../../../common/ContentIntroSection'

const res = await fetch(API_URL + 'blocks/773');
const data = await res.json();

const pageData = data[2];
const background='[bg-gradient-to-b from-[#FFFFFF] to-[#FEF2F2]]'
const colVal = [
  { value: 1 },
  { value: 3 },
  { value: 3 }
];
const combinedData = {...pageData,colVal,background};
---
<!-- Third Section -->
<ContentIntroSection SectionData={combinedData} client:load/>