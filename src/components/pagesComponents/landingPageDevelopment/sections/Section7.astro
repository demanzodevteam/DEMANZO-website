---
import HighlightSteps from "../../../../components/common/HighlightSteps";
import { API_URL } from '../../../../config/urls';


const res = await fetch(API_URL + 'blocks/773');
const json = await res.json();
const SectionData = json[6];
const colVal = [
  { value: 1 },
  { value: 2 },
  { value: 3 }
];

const combinedData ={...SectionData,colVal}
---
<!-- Seventh Section -->
<HighlightSteps SectionData={combinedData} client:load/>