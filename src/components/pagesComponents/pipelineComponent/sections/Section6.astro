---
import { API_URL } from "../../../../config/urls";
import FeatureSection from "../../../common/FeatureSection.jsx";

// Static or dynamic slug
const slug = "280"; 

const res = await fetch(`${API_URL}blocks/${slug}`);
const data = await res.json();
const pageData = data[5];
const background ="gradient-to-b from-[#FEF3F3] to-[#FEFCFC]"

const combinedData ={...pageData,background}
---
<!-- Sixth Section -->
 

<FeatureSection pageData={combinedData} />