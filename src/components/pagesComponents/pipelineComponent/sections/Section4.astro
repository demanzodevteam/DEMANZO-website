---
import { API_URL } from "../../../../config/urls";
import FeatureSection from "../../../common/FeatureSection.jsx";

// Static or dynamic slug
const slug = "280"; 

const res = await fetch(`${API_URL}blocks/${slug}`);
const data = await res.json();
const pageData = data[3];
const colVal = [
  { value: 1 },
  { value: 4 },
  { value: 3 }
];

const combinedData = {...pageData,colVal}

---
<!-- Four Section  -->
<FeatureSection pageData={combinedData} client:load/>





