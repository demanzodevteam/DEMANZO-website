---
import { API_URL } from "../../../../config/urls";
import GrowthAnimation from "../renderers/GrowthAnimation"

const res = await fetch(API_URL + 'category/3');
const category = await res.json();
---

<!-- -----------------section 3---------------- -->

<div class="py-8 mt-90 md:mt-[460px] lg:mt-20 xl:mt-32 2xl:mt-52 bg-gradient-to-b from-[#fef1ee] to-[#fcf8f9]">
  
    <GrowthAnimation category={category} client:visible />
</div>