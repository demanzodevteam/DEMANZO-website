---
import ComparisonTableSection from "../renderers/ComparisonTableSection.jsx";

// You can fetch from an API here instead if needed
const title = "Enterprise SEO vs. Traditional SEO";
const footerText = "Want the Enterprise SEO advantage? Let's make it happen!";

const tableHeadings = ["Feature", "Traditional SEO", "Enterprise SEO"];
const tableData = [
  ["Keyword Targeting", "Broad & general", "Niche, high-intent, competitive"],
  ["Content Strategy", "Basic blogs", "AI-powered, pillar clusters, technical content"],
  ["Link Building", "Generic backlinks", "High-authority industry links & digital PR"],
  ["Technical SEO", "Basic fixes", "Advanced site performance, structured data, schema markup"],
  ["Analytics & Optimization", "Manual updates", "AI-driven real-time tracking & testing"],
];
---

<ComparisonTableSection
  title={title}
  footerText={footerText}
  tableHeadings={tableHeadings}
  tableData={tableData}
/>
