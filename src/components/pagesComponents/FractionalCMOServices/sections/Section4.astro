const pageData = await res.json();

const background = 'bg-[#FFFFF]';
const colVal = [
  { value: 1 },
  { value: 2 }, 
  { value: 3 }
];
const noPoints = 'list-disc pl-0'
const combinedData = {
    ...pageData[3],
    background,colVal,noPoints
  };
